#include "main.h"

/**
 * more_numbers - prints all numbers from 0 till 14
 * Return: void (success)
 */

void more_numbers(void)
{
	int t,
	    i;

	for (t = 0; t < 10; t++)
	{
		for (i = 0; i <= 14; i++)
		{
			if ((i / 10) > 0)
			{
				putchar((i /10) + '0');
			}
			putchar(i % 10);
		};
		putchar('\n');
	};
}
